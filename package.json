    "@react-navigation/native-stack": "^6.9.26",
    "expo": "~54.0.0",
    "expo-location": "~19.0.8",
    "expo-status-bar": "~3.0.9",
    "firebase": "^10.12.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~19.1.10",
    "typescript": "^5.4.5"
  }
