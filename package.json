{
  "name": "khidmaty-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "env:sync": "node scripts/syncEnvFromWeb.mjs",
    "start": "expo start",
    "start:lan": "expo start --lan",
    "start:lan:ip": "node scripts/expoStartLan.mjs",
    "start:tunnel": "expo start --tunnel",
    "start:clean:lan": "expo start -c --lan",
    "start:clean:lan:ip": "node scripts/expoStartLan.mjs --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "export:web": "expo export -p web && node scripts/postExportWebFixes.mjs",
    "vercel-build": "npm run export:web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
